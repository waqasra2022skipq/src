    $sIndicated = $tL1 == 0 
                  && ($tL2 >= 0 && $tL2 <= 1) || ($tL2 >= 2 && $tL2 <= 3 && $rPHQ->{'D2q8'} == 1)
                  && ($tL3 >= 0 && $tL3 <= 1)
                  && ($tL5 >= 2 && $tL5 <= 4)
                  && ($tL6 >= 2 && $tL6 <= 3)
                  && ($tL4 >= 3 && $tL4 <= 3) ? '0.5'
                : ($tL1 >= 0 && $tL1 <= 1) || ($tL1 >= 2 && $tL1 <= 4 && $tWM == 1)
                  && ($tL2 >= 0 && $tL2 <= 1) || ($tL2 >= 2 && $tL2 <= 3 && $rPHQ->{'D2q8'} == 1)
                  && ($tL3 >= 0 && $tL3 <= 1) || ($tL3 >= 2 && $tL3 <= 3 && $rPHQ->{'D3q9'} == 1)
                  && ($tL5 >= 0 && $tL5 <= 2)
                  && ($tL6 >= 0 && $tL6 <= 2)
                  && ($tL4 >= 0 && $tL4 <= 2) || 
                     ($tL4 >= 3 && $tL4 <= 4 && $tL1 >= 0 && $tL1 <= 1 && $tL2 >= 0 && $tL2 <= 1 && $tL3 >= 0 && $tL3 <= 1 && $tL5 >= 0 && $tL5 <= 1 && $tL6 >= 0 && $tL6 <= 1) ? '1'
